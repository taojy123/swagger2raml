/api/medias/file/:
  post:
    body:
      application/json:
        type: WriteFileSerializer

    responses:
      200:
        body:
          application/json:
            type: FileSerializer

  get:
    queryParameters:
      page:
        required: false
        type: integer
      page_size:
        required: false
        type: integer
      file_type:
        required: false
        enum:
          - _folder
          - image
          - video
          - audio
          - other
      file_type__in:
        required: false
        type: string
      id:
        required: false
        type: string
      id__in:
        required: false
        type: string
      parent:
        required: false
        type: string
      order_by:
        required: false
        enum:
          - updated_at
          - -updated_at
          - created_at
          - -created_at
          - title
          - -title
          - title_pinyin
          - -title_pinyin
          - path
          - -path

    responses:
      200:
        body:
          application/json:
            type: FileSerializer[]


