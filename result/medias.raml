/api/medias/all_folders/:
  get:
    queryParameters:
      id:
        required: false
        type: string
      id__in:
        required: false
        type: string
      parent:
        required: false
        type: string
      order_by:
        required: false
        enum:
          - updated_at
          - -updated_at
          - created_at
          - -created_at
          - title
          - -title
          - title_pinyin
          - -title_pinyin
          - path
          - -path

    responses:
      200:
        body:
          application/json:
            type: FileSerializer


