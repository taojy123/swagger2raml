        required: false
        type: string
      price_from:
        required: false
        type: string
      price_to:
        required: false
        type: string
      collection:
        required: false
        type: string
      collection__exclude:
        required: false
        type: string
      category:
        required: false
        type: string
      category__in:
        required: false
        type: string
      q:
        required: false
        type: string
      order_by:
        required: false
        enum:
          - id
          - -id
          - created_at
          - -created_at
          - name
          - -name
          - inventory_quantity
          - -inventory_quantity
          - title
          - -title
          - price_min
          - -price_min
          - published_at
          - -published_at

    responses:
      200:
        body:
          application/json:
            schema: ProductSerializer[]



